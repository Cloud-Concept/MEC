    <fields>
        <fullName>Stage__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(RecordType.Name==&apos;RFPO&apos;,ISPICKVAL(Approval_Status__c, &apos;Not Approved&apos;)),&apos;RFPO&apos;, 
IF(AND(RecordType.Name==&apos;RFPO&apos;,ISPICKVAL(Approval_Status__c, &apos;Submitted&apos;)), &apos;RFPO Submitted&apos;, 
IF(AND(RecordType.Name==&apos;PO&apos;,ISPICKVAL(Approval_Status__c,&apos;Not Approved&apos;)),&apos;PO&apos;,
IF(AND(RecordType.Name==&apos;PO&apos;,ISPICKVAL(Approval_Status__c,&apos;Submitted&apos;)),&apos;PO Submitted&apos;,
IF(AND(RecordType.Name==&apos;PO&apos;,ISPICKVAL(Approval_Status__c, &apos;PO Approved&apos;)), &apos;PO-Filled&apos;,&apos;Rejected&apos;)))))</formula>
        <label>RFPO/PO Stage</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
